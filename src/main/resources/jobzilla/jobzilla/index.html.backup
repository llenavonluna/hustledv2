    <script>
        // This function waits until the entire HTML document is ready
        document.addEventListener('DOMContentLoaded', function() {

            // The code inside here will ONLY run after the button and popup exist on the page
            var chatToggler = document.getElementById('chat-toggler');
            var chatPopup = document.getElementById('librechat-popup-container');

            // This checks if the elements were successfully found
            if (chatToggler && chatPopup) {

                // Attach the click listener to the button
                chatToggler.addEventListener('click', function() {
                    // console.log("Chat button clicked! Toggling window."); // Debug line

                    // Toggle the 'open' class
                    if (chatPopup.classList.contains('open')) {
                        chatPopup.classList.remove('open');
                    } else {
                        chatPopup.classList.add('open');
                    }
                });
            }

            // ==================== AUTHENTICATION HANDLERS ====================

            // Handle Candidate Sign Up
            const candidateSignUpForm = document.getElementById('candidateSignUpForm');
            if (candidateSignUpForm) {
                candidateSignUpForm.addEventListener('submit', async function(e) {
                    e.preventDefault();
                    
                    const username = document.querySelector('#sign-candidate input[name="username"]').value;
                    const email = document.querySelector('#sign-candidate input[name="email"]').value;
                    const password = document.querySelector('#sign-candidate input[name="password"]').value;
                    const phone = document.querySelector('#sign-candidate input[name="phone"]').value;

                    try {
                        console.log('Submitting candidate signup form...', { username, email, password, phone });
                        const response = await fetch('http://localhost:8080/api/auth/signup/candidate', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                username: username,
                                email: email,
                                password: password,
                                phone: phone
                            })
                        });

                        console.log('Response status:', response.status);
                        const data = await response.json();
                        console.log('Response data:', data);
                        
                        if (data.success) {
                            alert('Registration successful! Please login.');
                            // Close signup modal and open login modal
                            document.getElementById('sign_up_popup').classList.remove('show');
                            document.getElementById('sign_up_popup2').classList.add('show');
                            candidateSignUpForm.reset();
                        } else {
                            alert('Registration failed: ' + data.message);
                        }
                    } catch (error) {
                        console.error('Error:', error);
                        alert('Error: ' + error.message);
                    }
                });
            }

            // Handle Employer Sign Up
            const employerSignUpForm = document.getElementById('employerSignUpForm');
            if (employerSignUpForm) {
                employerSignUpForm.addEventListener('submit', async function(e) {
                    e.preventDefault();
                    
                    const username = document.querySelector('#sign-Employer input[name="username"]').value;
                    const email = document.querySelector('#sign-Employer input[name="email"]').value;
                    const password = document.querySelector('#sign-Employer input[name="password"]').value;
                    const phone = document.querySelector('#sign-Employer input[name="phone"]').value;

                    try {
                        console.log('Submitting employer signup form...', { username, email, password, phone });
                        const response = await fetch('http://localhost:8080/api/auth/signup/admin', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                username: username,
                                email: email,
                                password: password,
                                phone: phone
                            })
                        });

                        console.log('Response status:', response.status);
                        const data = await response.json();
                        console.log('Response data:', data);
                        
                        if (data.success) {
                            alert('Registration successful! Please login.');
                            // Close signup modal and open login modal
                            document.getElementById('sign_up_popup').classList.remove('show');
                            document.getElementById('sign_up_popup2').classList.add('show');
                            employerSignUpForm.reset();
                        } else {
                            alert('Registration failed: ' + data.message);
                        }
                    } catch (error) {
                        console.error('Error:', error);
                        alert('Error: ' + error.message);
                    }
                });
            }

            // Handle Candidate Login
            const candidateLoginForm = document.getElementById('candidateLoginForm');
            if (candidateLoginForm) {
                candidateLoginForm.addEventListener('submit', async function(e) {
                    e.preventDefault();
                    
                    const username = document.querySelector('#login-candidate input[name="username"]').value;
                    const password = document.querySelector('#login-candidate input[name="password"]').value;

                    try {
                        console.log('Submitting candidate login form...', { username });
                        const response = await fetch('http://localhost:8080/api/auth/login/candidate', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                username: username,
                                password: password
                            })
                        });

                        console.log('Response status:', response.status);
                        const data = await response.json();
                        console.log('Response data:', data);
                        
                        if (data.success) {
                            // Store user info in localStorage
                            localStorage.setItem('user', JSON.stringify({
                                id: data.userId,
                                username: data.username,
                                email: data.email,
                                role: data.role
                            }));
                            alert('Login successful!');
                            // Close modal and redirect
                            document.getElementById('sign_up_popup2').classList.remove('show');
                            candidateLoginForm.reset();
                            // Redirect to candidate dashboard
                            window.location.href = '/candidate-dashboard.html';
                        } else {
                            alert('Login failed: ' + data.message);
                        }
                    } catch (error) {
                        console.error('Error:', error);
                        alert('Error: ' + error.message);
                    }
                });
            }

            // Handle Employer Login
            const employerLoginForm = document.getElementById('employerLoginForm');
            if (employerLoginForm) {
                employerLoginForm.addEventListener('submit', async function(e) {
                    e.preventDefault();
                    
                    const username = document.querySelector('#login-Employer input[name="username"]').value;
                    const password = document.querySelector('#login-Employer input[name="password"]').value;

                    try {
                        console.log('Submitting employer login form...', { username });
                        const response = await fetch('http://localhost:8080/api/auth/login/admin', {
                            method: 'POST',
                            headers: {
                                'Content-Type': 'application/json'
                            },
                            body: JSON.stringify({
                                username: username,
                                password: password
                            })
                        });

                        console.log('Response status:', response.status);
                        const data = await response.json();
                        console.log('Response data:', data);
                        
                        if (data.success) {
                            // Store user info in localStorage
                            localStorage.setItem('user', JSON.stringify({
                                id: data.userId,
                                username: data.username,
                                email: data.email,
                                role: data.role
                            }));
                            alert('Login successful!');
                            // Close modal and redirect
                            document.getElementById('sign_up_popup2').classList.remove('show');
                            employerLoginForm.reset();
                            // Redirect to employer dashboard
                            window.location.href = '/dash-employer.html';
                        } else {
                            alert('Login failed: ' + data.message);
                        }
                    } catch (error) {
                        console.error('Error:', error);
                        alert('Error: ' + error.message);
                    }
                });
            }
        });
    </script>
